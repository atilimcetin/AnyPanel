{
    "opacity": 0.65,
    "font-family": "Lucida Console, Monaco, monospace",
    "font-size": "10px",
    "geometry": {
        "x": %d,
        "y": %d,
        "width": %d,
        "height": %d
    },
    "panels": [{
        "name": "general",
        "rows": 2,
        "columns": 4,
        "color": "black",
        "background-color": "lightgreen",
        "color-2": "black",
        "background-color-2": "lightblue",
        "color-alternating-rows": false,
        "color-alternating-columns": true,
        "border": "1px solid black"
    }],
    "scripts": [{
        "cell": "1,1",
        "frequency": "10s",
        "command": "/Applications/AnyPanel.app/Contents/Scripts/disk-usage.sh"
    }, {
        "cell": "1,2",
        "frequency": "10m",
        "command": "/Applications/AnyPanel.app/Contents/Scripts/external-ip.sh"
    }, {
        "cell": "2,2",
        "frequency": "1m",
        "command": "/Applications/AnyPanel.app/Contents/Scripts/local-ip.sh"
    }, {
        "cell": "1,4",
        "frequency": "1s",
        "command": "/Applications/AnyPanel.app/Contents/Scripts/ping-google-dns.sh"
    }, {
        "cell": "2,1",
        "frequency": "10s",
        "command": "/Applications/AnyPanel.app/Contents/Scripts/smiley.sh"
    }, {
        "cell": "2,4",
        "frequency": "10s",
        "command": "/Applications/AnyPanel.app/Contents/Scripts/wifiname.sh"
    }]
}
